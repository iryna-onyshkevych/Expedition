@model Excursion.Models.Review
@{
    ViewBag.Title = "Adding Coach";
}

<div id="modDialog" class="modal fade">
    <div id="dialogContent" class="modal-dialog"></div>
</div>
<div class="modal-content">
    <div class="modal-header">
    </div>
    <div class="modal-body">
        <form asp-action="Create" asp-controller="Review">

            <div class="form-group">
                <label asp-for="ParticipantName" class="control-label">Ваше ім'я:</label>
                <input type="text" asp-for="ParticipantName" class="form-control" />
                <span asp-validation-for="ParticipantName" class="text-danger" />
            </div>

            <div class="form-group">
                <label asp-for="ParticipantReview" class="control-label">Відгук:</label>
                <input type="text" asp-for="ParticipantReview" class="form-control" />
                <span asp-validation-for="ParticipantReview" class="text-danger" />
            </div>

            <div class="form-group">
                <input type="submit" value="Додати" class="btn btn-info" />
            </div>

        </form>



    </div>
</div>
@section scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}